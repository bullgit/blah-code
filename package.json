  "files": [
    "index.js",
    "blah-code.js",
    "blah-code.min.js",
    "blah-code.min.js.map"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "posttest": "npm run lint",
    "build": "uglifyjs blah-code.js --source-map blah-code.min.js.map --source-map-url blah-code.min.js.map --preamble \"/*! ${npm_package_name} - v${npm_package_version} - ${npm_package_homepage} - ${npm_package_license} License */\" --mangle --compress --output blah-code.min.js",
    "release:major": "mversion major",
    "release:minor": "mversion minor",
    "release:patch": "mversion patch"
  },
  "keywords": [
    "blah",
    "bullgit"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "mversion": "^1.10.1",
    "uglify-js": "^2.6.1",
    "xo": "^0.12.1"
  },
  "xo": {
    "envs": [
      "node",
      "amd",
      "mocha"
    ],
    "rules": {
      "no-return-assign": [
        2,
        "except-parens"
      ]
    }
  }
}
